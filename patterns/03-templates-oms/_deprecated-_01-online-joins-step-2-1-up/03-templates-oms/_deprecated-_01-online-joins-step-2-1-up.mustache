{{> patterns-header-prospect }}
<main class="page-content main-mylt">
	<div class="container">
		<div class="row">
			<div class="col-xs-12">
				<div class="content-section-xs">
					<ol class="list-ordered centered mylt-gray-4 m-b-0">
						<li>Iâ€™m ready to join <strong>[clubname]</strong> <a href="{{link.templates-online-joins-step-1}}" class="small">Edit</a></li>
					</ol>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<hr class="b-thick b-clr-gray-4 m-t-0">
				<h1 class="h-scaled text-xs-center p-t-1"><span class="sr-only">Step </span><span class="coin coin-ring m-r-1">2</span>Membership Options</h1>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				{{#oneUp}}
				<div class="card-price-group one-up">
					{{> patterns-price-card(cardTitle: "Monthly", cardDescription: "Recurring month-to-month membership.<a id='referenceMonthly-ref' href='#viewTerms' aria-describedby='#referenceMonthly' class='mylt-gray-5'>*<span class='sr-only'>View terms and conditions</span></a>", badge: false, priceDescriptor: "Starting at", price: "99", priceFootnote: "Joining fees may apply", cta: "Select", url: "{{link.templates-online-joins-step-3}}", disclaimer: false) }}
				</div>
				{{/oneUp}}
				{{#twoUp}}
				<div class="card-price-group two-up">
					{{> patterns-price-card(cardTitle: "Monthly", cardDescription: "Recurring month-to-month membership.<a id='referenceMonthly-ref' href='#viewTerms' aria-describedby='#referenceMonthly' class='mylt-gray-5'>*<span class='sr-only'>View terms and conditions</span></a>", badge: false, priceDescriptor: "Starting at", price: "99", priceFootnote: "Joining fees may apply", cta: "Select", url: "{{link.templates-online-joins-step-3}}", disclaimer: false) }}
					{{> templates-online-joins-free-pass }}
				</div>
				{{/twoUp}}
				{{#threeUp}}
				<div class="card-price-group three-up">
					{{> patterns-price-card(cardTitle: "Monthly", cardDescription: "Recurring month-to-month membership.<a id='referenceMonthly-ref' href='#viewTerms' aria-describedby='#referenceMonthly' class='mylt-gray-5'>*<span class='sr-only'>View terms and conditions</span></a>", badge: false, priceDescriptor: "Starting at", price: "99", priceFootnote: "Joining fees may apply", cta: "Select", url: "{{link.templates-online-joins-step-3}}", disclaimer: false) }}
					{{> patterns-price-card(cardTitle: "Day Pass", cardDescription: "Full club access for the day.<a id='referenceDayPass-ref' href='#viewTerms' aria-describedby='#referenceDayPass'>*<span class='sr-only'>View terms and conditions</span></a>", badge: false, priceGroup: false, priceModifier: "price-lockup-sm b-t b-b p-t-sm m-b-4", priceGroupCustom: true, priceCustom: "<form class='text-xs-left'><fieldset class='form-group'><label class='c-input c-radio m-r-0'><input name='pass-prices' type='radio' aria-labelledby='price-single' checked='checked'><span class='c-indicator'></span><span id='price-single' class='currency'>$</span><span class='value'>25</span><span class='unit'> / 1 pass</span></label><label class='c-input c-radio m-r-0'><input name='pass-prices' type='radio' aria-labelledby='price-multiple'><span class='c-indicator'></span><span id='price-multiple' class='currency'>$</span><span class='value'>125</span><span class='unit'> / 5 passes</span></label></fieldset></form>", cta: "Select", url: "{{link.templates-online-joins-step-3}}", disclaimer: false) }}
					{{> templates-online-joins-info }}
				</div>
				{{/threeUp}}
				{{#fourUp}}
				<div class="card-price-group four-up">
					{{> patterns-price-card(cardTitle: "Monthly", cardDescription: "Recurring month-to-month membership.<a id='referenceMonthly-ref' href='#viewTerms' aria-describedby='#referenceMonthly' class='mylt-gray-5'>*<span class='sr-only'>View terms and conditions</span></a>", badge: false, priceDescriptor: "Starting at", price: "99", priceFootnote: "Joining fees may apply", cta: "Select", url: "{{link.templates-online-joins-step-3}}", disclaimer: false) }}
					{{> patterns-price-card(cardTitle: "Annual", cardDescription: "Lock in a low rate for a year, then go month-to-month.<a id='referenceAnnual-ref' href='#viewTerms' aria-describedby='#referenceAnnual' class='mylt-gray-5'>*<span class='sr-only'>View terms and conditions</span></a>", badge: "Best Value", priceDescriptor: "Starting at", price: "79", priceFootnote: "Joining fees may apply", cta: "Select", url: "{{link.templates-online-joins-step-3-Good-Better-Best}}", disclaimer: false) }}
					{{> patterns-price-card(cardTitle: "Day Pass", cardDescription: "Full club access for the day. <a id='referenceDayPass-ref' href='#viewTerms' aria-describedby='#referenceDayPass'>*<span class='sr-only'>View terms and conditions</span></a>", badge: false, priceGroup: false, priceModifier: "price-lockup-sm b-t b-b p-t-sm m-b-4", priceGroupCustom: true, priceCustom: "<form class='text-xs-left'><fieldset class='form-group'><label class='c-input c-radio m-r-0'><input name='pass-prices' type='radio' aria-labelledby='price-single' checked='checked'><span class='c-indicator'></span><span id='price-single' class='currency'>$</span><span class='value'>25</span><span class='unit'> / 1 pass</span></label><label class='c-input c-radio m-r-0'><input name='pass-prices' type='radio' aria-labelledby='price-multiple'><span class='c-indicator'></span><span id='price-multiple' class='currency'>$</span><span class='value'>125</span><span class='unit'> / 5 passes</span></label></fieldset></form>", cta: "Select", url: "{{link.templates-online-joins-step-3}}", disclaimer: false) }}
					{{> templates-online-joins-free-pass }}
				</div>
				{{/fourUp}}
			</div>
		</div>

		<div class="row">
			<div class="col-xs-12 col-md-10 col-md-offset-1 col-lg-8 col-lg-offset-2 text-xs-center">
				{{> templates-online-joins-legal }}
			</div>
		</div>

		<div class="row">
			<div class="col-xs-12">
				<hr class="b-thick">
				<div class="h2 h-static text-xs-center text-disabled m-b-0"><span class="sr-only">Step </span><span class="coin coin-ring m-r-1">3</span>Build Membership</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<hr class="b-thick">
				<div class="h2 h-static text-xs-center text-disabled m-b-0"><span class="sr-only">Step </span><span class="coin coin-ring m-r-1">4</span>Member Info</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<hr class="b-thick">
				<div class="h2 h-static text-xs-center text-disabled m-b-2"><span class="sr-only">Step </span><span class="coin coin-ring m-r-1">5</span>Payment</div>
			</div>
		</div>
	</div>
</main>
{{> patterns-prospect-footer }}