<li class="map-result b-b p-a-grid" tabindex="0" aria-title="{{clubName}}" aria-role="button">
	<div class="row">
		<div class="col-xs-8 col-lg-7">
			{{#waitList}}
				<p class="h-th">Opening Fall 2017</p>
			{{/waitList}}
			<p id="{{cityID}}" class="text-gray-4 font-weight-bold m-b-0">{{{clubName}}}</p>
			<div class="small">
				<p class="m-b-0 text-gray-4"><span class="font-weight-bold">{{disMile}}</span> away<span class="hidden-lg-up"> <a href="#0">Directions</a></span></p>
				<p class="m-b-0 hidden-lg-up"><a href="#0">(952) 829-8400</a></p>
				<p class="m-b-0">{{clubLevel}} Club</p>
				{{^memberSite}}
					<p class="m-b-0">Starting at {{clubPrice}}/mo</p>
				{{/memberSite}}
			</div>
		</div>
		<div class="col-xs-4 col-lg-5 edge-l-xs-up text-xs-right">
			{{^memberSite}}
				{{#waitList}}
					<a href="#" aria-describedby="{{cityID}}" class="btn btn-primary btn-xs m-l-sm m-b-sm pull-xs-right">Wait List</a>
				{{/waitList}}
				{{^waitList}}
					<a href="#" aria-describedby="{{cityID}}" class="btn btn-primary btn-xs m-l-sm m-b-sm pull-xs-right">Join</a>
				{{/waitList}}
			{{/memberSite}}
			<a href="#" aria-describedby="{{cityID}}" class="btn btn-link btn-xs m-b-sm p-x-0 b-x-xs-0 pull-xs-right">Club Details</a>
		</div>
	</div>
</li>
