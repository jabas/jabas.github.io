{{> templates-bios-admin-header }}
<main>
    <form>
        <div class="p-t-2 container">
            <div class="row">
                <div class="col-xs-12 col-md-10 col-md-offset-1 col-lg-12 col-lg-offset-0">
                    <h1 class="h-scaled m-b-2 text-xs-center"><span class="sr-only">Step </span><span class="coin coin-ring m-r-1">1</span>Create Notification</h1>
                    <div class="row row-xs-bottom">
                        <div class="col-xs-12 col-lg-5">
                            <p class="font-italic m-b-sm">All fields required</p>
                        </div>
                        <div class="col-xs-12 col-lg-7 small">
                            <p class="m-b-sm">Need help? Read the <a href="https://ltfinc.sharepoint.com/DepartmentPortals/MemberServices/Documents/Club%20Notifications%20Guide.pdf" target="_blank">Club Notifications Guide</a>.</p>
                        </div>
                    </div>
                    <div class="p-y-2 b-t">
                        <div class="row">
                            <div class="col-xs-12 col-lg-5">
                                <h2 class="h5 m-b-grid">Notification Type</h2>
                            </div>
                            <div class="col-xs-12 col-lg-7">
                                <div class="form-group m-b-0">
                                    {{#multiclub}}
                                    <label for="selType">Notification Type</label>
                                    {{/multiclub}}
                                    {{^multiclub}}
                                    <label for="selType" class="sr-only">Notification Type</label>
                                    {{/multiclub}}
                                    <select id="selType" class="c-select">
                                        <option>Select a type...</option>
                                        {{#multiclub}}
                                        <optgroup label="Single Location">
                                        {{/multiclub}}
                                        <option>Full Club Closure</option>
                                        <option>Full Club Re-Opening</option>
                                        <option>Modified Club Hours</option>
                                        <option>Modified Amenity Hours</option>
                                        <option>Locker Room Closure</option>
                                        <option>Child Center Closure</option>
                                        <option>Full Amenity Closure (e.g. Studios, Gym, Pools, Fitness Floor)</option>
                                        <option>Partial Amenity Closure (e.g. Studios, Gym, Pools, Fitness Floor)</option>
                                        <option>Event Cancellation</option>
                                        <option>Other</option>
                                        {{#multiclub}}
                                        </optgroup>
                                        <optgroup label="Multiple Location">
                                            <option value="all">All Locations</option>
                                            <option value="multi">Select Locations</option>
                                        </optgroup>
                                        {{/multiclub}}
                                    </select>
                                </div>
                                {{#multiclub}}
                                <div id="colStyle" class="collapse fade" aria-expanded="false">
                                    <div class="form-group p-t-1 m-b-0">
                                        <label for="selStyle">Styling</label>
                                        <select id="selStyle" class="c-select">
                                            <option selected>Danger (Red)</option>
                                            <option>Warning (Yellow)</option>
                                            <option>Success (Green)</option>
                                            <option>Info (Blue)</option>
                                        </select>
                                    </div>
                                </div>
                                {{/multiclub}}
                            </div>
                        </div>
                    </div>
