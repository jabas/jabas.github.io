{{> patterns-header-member }}
<main id="main-content" role="main" class=" main-mylt">
   <div id="myacEditEmail">
      <div class="lt-edit-email">
         <div>
            <!---->
         </div>
         <!----> 
         <div class="mylt-bg-gray-12 p-t-2 p-b-3">
            <div class="container">
               <nav class="hide-print">
                  <ol class="breadcrumb p-y-0">
                     <li><a href="/account.html">Account</a></li>
                     <!----> 
                     <li>LifeCafe Rewards</li>
                  </ol>
               </nav>
               <div id="card_edit_email" class="card card-index card-account">
                  <span class="ico-user user-icon hide-print"></span> 
                  <div class="card-header hide-print">
                     <h2 id="title_edit_email" class="h5 card-title">LifeCafe Rewards</h2>
                  </div>
                  <div class="card-block lcr_card-block">
                     <div class="row">
                        <div id="instructions_container" class="col-xs-12 col-md-offset-2 col-md-8 col-lg-offset-0 col-lg-4 m-b-1">
                        	<div class="row m-b-2">
                        		<div class="col-xs-12 edge-sm-down">
                        			<picture class="hero-banner2">
										<source media="(min-width:992px)" srcset="https://my.lifetime.life/content/dam/mylt/images/cafe/cafe-masonry-xl.jpg">
										<img alt="Booth seating and ordering counter at a Life Cafe" src="https://my.lifetime.life/content/dam/mylt/images/cafe/cafe-masonry-xs.jpg">
									</picture>
                        		</div>
                        	</div>
                        	
                            <h3 class="h5 m-b-2 hidden-lg-up">Track Eligible LifeCafe Purchases</h3>
                            <p class="m-b-grid">After the 10th purchase, the 11th item is complimentary. See<span class="hidden-lg-up"> below for</span> details<span class="hidden-md-down"> at right</span>.</p>
                        </div>
                        <div id="input_container" class="col-xs-12 col-md-offset-2 col-md-8 col-lg-offset-1 col-lg-5">
                        	<h3 class="h5 m-b-2 hidden-md-down m-b-sm">Track Eligible LifeCafe Purchases</h3>
                        	<div class="p-y-grid b-y">
                        		<div class="row m-b-sm">
                        			<div class="col-xs-8">Shakes/Smoothies</div>
                        			<div class="col-xs-4 text-xs-right"><a href="#shakeReward" data-toggle="collapse" role="button">Redeem</a></div>
                        		</div>
								<div class="progress">
									<div class="progress-bar" role="progressbar" style="width: 100%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="10"></div>
								</div>
								<div id="shakeReward" class="collapse">
									<div class="content-section text-xs-center">
										<img src="/images/styleguide/lifecafe-qr.svg" alt="" class="m-b-2">
										<img src="/images/styleguide/add-to-wallet.svg" alt="" class="m-b-sm">
									</div>
								</div>
							</div>
							<div class="p-y-grid b-y">
                        		<div class="row m-b-sm">
                        			<div class="col-xs-8">Entrees*</div>
                        			<div class="col-xs-4 text-xs-right">5/10</div>
                        		</div>
                        		<div class="progress">
									<div class="progress-bar" role="progressbar" style="width: 50%" aria-valuenow="5" aria-valuemin="0" aria-valuemax="10"></div>
								</div>
							</div>
							<div class="p-y-grid b-y">
                        		<div class="row m-b-sm">
                        			<div class="col-xs-8">Meals To Go</div>
                        			<div class="col-xs-4 text-xs-right">3/10</div>
                        		</div>
                        		<div class="progress">
									<div class="progress-bar" role="progressbar" style="width: 30%" aria-valuenow="3" aria-valuemin="0" aria-valuemax="10"></div>
								</div>
							</div>
							<div class="p-y-grid b-y">
                        		<div class="row m-b-sm">
                        			<div class="col-xs-8">Coffee</div>
                        			<div class="col-xs-4 text-xs-right">0/10</div>
                        		</div>
                        		<div class="progress">
									<div class="progress-bar" role="progressbar" style="width: 0%" aria-valuenow="3" aria-valuemin="0" aria-valuemax="10"></div>
								</div>
							</div>
                           
							<div class="content-section small">
                           		<p>Value of complimentary item is limited:</p>
                           		<table class="table-simple">
                           			<tbody>
                           				<tr>
                           					<th class="p-r-2">Shake/Smoothie:</th>
                           					<td>$XX.XX</td>
                           				</tr>
                           				<tr>
                           					<th class="p-r-2">Entree:</th>
                           					<td>$XX.XX</td>
                           				</tr>
                           				<tr>
                           					<th class="p-r-2">Meals To Go:</th>
                           					<td>$XX.XX</td>
                           				</tr>
                           				<tr>
                           					<th class="p-r-2">Coffee:</th>
                           					<td>$XX.XX</td>
                           				</tr>
                           			</tbody>
                           		</table>
								<p>*Entree = Breakfast, Salad, Sandwich, Wrap, Quesadilla, Taco or Pizza</p>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</main>
{{> patterns-mylt-footer }}