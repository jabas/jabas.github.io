<!-- SUNDAY -->
<div class="p-t-grid p-b-sm b-b">
	<div class="row row-xs-bottom">
		<div class="col-xs-12 col-lg-2">
			<div class="lsot-hours-label">
				Sunday
			</div>
		</div>
		<div class="col-xs-12 col-sm-8 col-md-5 col-lg-4">
			<div class="form-group m-b-sm">
				<label class="sr-only" for="scheduleSetting">Setting</label>
				<select id="scheduleSetting" class="c-select">
					<option>Select One</option>
					<option>Set Specific Hours</option>
					<option>24 Hours</option>
					<option>Sunrise/Sunset</option>
					<option>Closed</option>
				</select>
			</div>
		</div>
		<div class="col-xs-9 col-sm-8 col-md-5 col-lg-4">
			<div class="lsot-hours-inputs m-b-sm">
				<div class="lsot-hours-input">
					{{> templates-location-timepicker(id:"SunOpen",label:"Opens",disabled:true) }}
				</div>
				<div class="lsot-hours-divider">
					<span class="hidden-sm-up">&ndash;</span>
					<span class="hidden-xs-down">&mdash;</span>
				</div>
				<div class="lsot-hours-input">
					{{> templates-location-timepicker(id:"SunClose",label:"Closes",disabled:true) }}
				</div>
			</div>
		</div>
		<div class="col-xs-3 col-sm-4 col-md-2 p-l-0">
			<div class="btn-wrap m-b-sm">
				<button disabled class="btn btn-link p-x-0"><span class="ico-plus-sm icon-adjust-horiz"></span><span class="btn-icon-text">Add</span></button>
			</div>
		</div>
	</div>
</div>
