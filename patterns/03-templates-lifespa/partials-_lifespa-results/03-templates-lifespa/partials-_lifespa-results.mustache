<div class="planner">
	{{> templates-lifespa-datebar }}
	<div class="container">
		{{#dayparts}}
			<div>
				<div class="planner-daypart planner-daypart-{{name}}">
					<div class="planner-daypart-leadin">
						Starts at
					</div>
					<div class="planner-daypart-label">
						{{{name}}}
					</div>
				</div>
				{{#times}}
					<div class="planner-times">
						<div class="planner-time">
							{{{name}}}
						</div>
						{{#slots}}
							<div class="planner-entry">
								<div class="planner-row">
									<div class="planner-col" style="flex: 4 0 0;">
										<div class="planner-entry-title">
											<a href="#0">{{serviceName}}</a> 
										</div>
									</div>
									<div class="planner-col font-weight-bold text-md-right" style="flex: 0 0 6em;">
										{{duration}} min
									</div>
									<div class="planner-col" style="flex: 2 0 0;">
										{{name}}
									</div>
									<div class="planner-col" style="flex: 3 0 0;">
										Chanhassen
									</div>
									<div class="planner-col font-weight-bold text-md-right" style="flex: 0 0 5em;">
										${{price}}
									</div>
									<div class="planner-entry-cta planner-col text-lg-center">
										<a href="#0">Book</a>
									</div>
								</div>
							</div>
						{{/slots}}
					</div>
				{{/times}}
			</div>
		{{/dayparts}}
	</div>
</div>