<h5 class="m-t-3">Align Image Left (Mobile and Up)</h5>
<div class="billboard mylt-bg-gray-4">
	<div class="billboard-content container-fluid content-section content-inverse">
		<div class="row">
			<div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-6 col-md-offset-6 col-lg-5 col-lg-offset-6 text-xs-center text-md-left">
				<h1 class="h-static">{{headline.short}}</h1>
				<p>{{excerpt.medium}}</p> 
			</div>
		</div>
	</div>
	<div class="billboard-img billboard-img-left-xs">
		{{> elements-billboard-img }}
	</div>
</div>
