<!-- FOR FUTURE ENHANCEMENTS - NOT CURRENTLY IN USE -->
<div class="modal fade" id="{{clubID}}" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="container p-t-grid">
				<div class="text-xs-right m-b-grid">
					<button type="button" class="btn btn-link btn-link-simple" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true" class="ico-close-sm"></span>
					</button>
				</div>

				<div class="row">
					<div class="col-xs-8 col-xl-7">
						<div class="text-gray-4">
							{{#waitList}}
							<p class="h-th">Opening Fall 2017</p>
							{{/waitList}}
							<p class="font-weight-bold m-b-0">{{{clubName}}}</p>
							<p class="small m-b-grid">{{clubLevel}} Club</p>
						</div>
					</div>
					<div class="col-xs-4 col-xl-5 text-xs-right">
						{{#waitList}}
							<button class="btn btn-primary btn-xs m-l-sm m-b-sm">Wait List</button>
						{{/waitList}}
						{{^waitList}}
							<button class="btn btn-primary btn-xs m-l-sm m-b-sm">Select</button>
						{{/waitList}}
					</div>
				</div>
				
				<div class="small">
					<div class="row m-b-grid">
						<div class="col-xs-12 col-sm-7">
							<p class="m-b-0 text-gray-4"><span class="font-weight-bold">{{disTime}}</span> away ({{disMile}})</p>
							<p class="m-b-0">755 Prarie Center Dr<br>Eden Prairie, MN 55344</p>
						</div>
						<div class="col-xs-12 col-sm-5 text-sm-right">
							<p class="m-b-0"><a href="#0">Get Directions</a></p>
							<p class="m-b-0"><a href="#0">(952) 380-0303</a></p>
						</div>
					</div>
					<p class="text-gray-4 font-weight-bold m-b-grid">Features</p>
					<div class="row m-b-grid">
						<div class="col-xs-12 col-sm-6">
							<ul class="list-dashed m-b-sm">
								<li>Group Fitness Classes</li>
								<li>Personal &amp; Group Training</li>
								<li>Kids Programming</li>
							</ul>
						</div>
						<div class="col-xs-12 col-sm-6">
							<ul class="list-dashed m-b-sm">
								<li>Indoor/Outdoor Pools</li>
								<li>LifeSpa</li>
								<li>LifeCafe</li>
							</ul>
						</div>
					</div>
					<p class="text-gray-4 font-weight-bold m-b-grid">Hours</p>
					<div class="row">
						<div class="col-xs-12 edge-xs-up">
							<table class="table table-sm table-striped m-b-0" summary="open hours summary">
								<caption class="sr-only">Club hours</caption>
								<thead class="sr-only">
									<tr>
										<th scope="col">Day</th>
										<th scope="col">Hours</th>
									</tr>
								</thead>
								<tbody>
									<tr>
										<td class="p-l-grid">Sunday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
									<tr>
										<td class="p-l-grid">Monday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
									<tr>
										<td class="p-l-grid">Tuesday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
									<tr>
										<td class="p-l-grid">Wednesday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
									<tr>
										<td class="p-l-grid">Thursday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
									<tr>
										<td class="p-l-grid">Friday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
									<tr>
										<td class="p-l-grid">Saturday</td>
										<td class="p-r-grid text-xs-right text-sm-left text-lg-right">24 hours</td>
									</tr>
								</tbody>
							</table>
						</div>
					</div>
					<div class="p-y-3">
						<button class="btn btn-primary btn-xs">Club Page</button>
						<button class="btn btn-primary btn-xs">Join This Club</button>
						<a href="#" role="button" class="m-a-sm sr-only sr-only-focusable" data-dismiss="modal">Close This Panel</a>
					</div>
					
				</div>				

			</div>
		</div>
	</div>
</div>