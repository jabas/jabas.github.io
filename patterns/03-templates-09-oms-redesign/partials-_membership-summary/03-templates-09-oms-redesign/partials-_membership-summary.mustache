<div class="omsc-summary-panel b-b b-l-lg b-r-lg b-t-lg p-a-grid box-shadow bg-white">
	<div class="omsc-summary-bar">
		<div class="omsc-summary-steps">
			<span class="omsc-step omsc-step-complete">
				{{> templates-icon-club }}
				<span class="omsc-step-text">Club Location</span>
			</span>
			<span class="omsc-step {{memberStepStatus}}">
				{{> templates-icon-person }}
				<span class="omsc-step-text">Members</span>
			</span>
			<span class="omsc-step {{interestStepStatus}}">
				{{> templates-icon-star }}
				<span class="omsc-step-text">Interests</span>
			</span>
			<span class="omsc-step {{membershipStepStatus}}">
				{{> templates-icon-membership }}
				<span class="omsc-step-text">Memberships</span>
			</span>
			<span class="omsc-step {{receiptStepStatus}}">
				{{> templates-icon-receipt }}
				<span class="omsc-step-text">Dues Reduction</span>
			</span>
		</div>
		<div class="omsc-summary-price">
			<div class="display-number display-number-md text-gray-3">
				<span class="sr-only">$ {{priceSummary}} per month</span>
				<div aria-hidden="true">
					<span class="display-number-sup">$</span><span class="display-number-value">{{priceSummary}}</span><span class="display-number-units">/mo</span>
				</div>
			</div>
		</div>
		<div class="omsc-summary-toggle">
			<a data-toggle="collapse" href="#selectionDetails" aria-expanded="false" aria-controls="selectionDetails" role="button" data-anchor-button="true">
				<span class="ico-arrow-down"></span><span class="sr-only">Show Selection Details</span>
			</a>
		</div>
	</div>
	<div id="selectionDetails" class="omsc-details-panel bg-white collapse m-b-6">
		<div class="omsc-details-column b-b b-l-lg b-r-lg b-t-lg b-rounded-lg bg-white box-shadow">
			<div class="omsc-details-header b-b p-a-grid box-shadow">
				<div class="row row-xs-center">
					<div class="col-xs-8 col-xs-offset-2 col-md-10 col-md-offset-0 text-xs-center text-md-left">
						<h2 class="omsc-details-heading h-static m-b-0">Membership Summary</h2>
					</div>
					<div class="col-xs-2 text-xs-right">
						<a data-toggle="collapse" href="#selectionDetails" aria-expanded="false" aria-controls="selectionDetails" role="button" data-anchor-button="true">
							<span class="ico-close hidden-md-up"></span><span class="ico-arrow-up hidden-sm-down"></span><span class="sr-only">Hide Selection Details</span>
						</a>
					</div>
				</div>
			</div>
			<div class="omsc-details-content m-t-grid">
				<div class="omsc-scroll">
					{{> templates-expanded-mbr-summary }}
				</div>
			</div>
		</div>
	</div>
</div>
