<li class="club-result p-a-grid">
	<div class="row">
		<div class="col-xs-8 col-xl-7">
			<div class="text-gray-4">
				{{#waitList}}
				<p class="h-th">Opening Fall 2017</p>
				{{/waitList}}
				<p id="{{cityID}}" class="font-weight-bold m-b-0">{{{clubName}}}</p>
			</div>
			<div class="small">
				<p class="m-b-0">Xxxxxxxx Club</p>
				<p class="m-b-0">Starting at $XX/mo</p>
			</div>
		</div>
		<div class="col-xs-4 col-xl-5 edge-l-xs-up text-xs-right">
			{{#waitList}}
				<a href="#" aria-describedby="{{cityID}} {{stateID}}" class="btn btn-primary btn-xs m-l-sm m-b-sm pull-xs-right">Wait List</a>
			{{/waitList}}
			{{^waitList}}
				<a href="#" aria-describedby="{{cityID}} {{stateID}}" class="btn btn-primary btn-xs m-l-sm m-b-sm pull-xs-right">Join</a>
			{{/waitList}}
			<a href="#" aria-describedby="{{cityID}} {{stateID}}" class="btn btn-link btn-xs m-b-sm p-x-0 b-x-xs-0 pull-xs-right">Club Details</a>
		</div>
	</div>
</li>
